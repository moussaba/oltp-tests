<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-exdep-viewer.xsl" ?>
<PhoronixTestSuite>
	<ExternalDependencies>
		<Information>
			<Name>Ubuntu</Name>
			<Aliases>Linux Mint, MEPIS, Debian, Nexenta Core, PTS Linux Live, PTS Desktop Live, Joli Cloud, gNewSense, Fluxbuntu, Mythbuntu, Goobuntu, GNU KFreeBSD</Aliases>
		</Information>
		<Package>
			<GenericName>32bit-compatibility</GenericName>
			<PackageName>ia32-libs</PackageName>
			<ArchitectureSpecific>x86_64</ArchitectureSpecific>
			<FileCheck>/usr/share/doc/ia32-libs</FileCheck>
		</Package>
		<Package>
			<GenericName>gtk-development</GenericName>
			<PackageName>libgtk2.0-dev</PackageName>
			<FileCheck>/usr/include/gtk-2.0</FileCheck>
		</Package>
		<Package>
			<GenericName>sdl-development</GenericName>
			<PackageName>libsdl1.2-dev libsdl-gfx1.2-dev libsdl-net1.2-dev libsdl-image1.2-dev libsdl-ttf2.0-dev libsdl-mixer1.2-dev</PackageName>
			<FileCheck>/usr/include/SDL/SDL.h, /usr/include/SDL/SDL_gfxPrimitives.h, /usr/include/SDL/SDL_net.h, /usr/include/SDL/SDL_image.h, /usr/include/SDL/SDL_ttf.h</FileCheck>
		</Package>
		<Package>
			<GenericName>glut</GenericName>
			<PackageName>freeglut3-dev</PackageName>
			<FileCheck>/usr/lib/libglut.so.3, /usr/include/GL/glut.h</FileCheck>
		</Package>
		<Package>
			<GenericName>csh</GenericName>
			<PackageName>csh</PackageName>
			<FileCheck>/bin/bsd-csh</FileCheck>
		</Package>
		<Package>
			<GenericName>libpng-development</GenericName>
			<PackageName>libpng12-dev</PackageName>
			<FileCheck>/usr/include/libpng12/png.h</FileCheck>
		</Package>
		<Package>
			<GenericName>build-utilities</GenericName>
			<PackageName>build-essential</PackageName>
			<FileCheck>/usr/share/build-essential</FileCheck>
		</Package>
		<Package>
			<GenericName>cairo-development</GenericName>
			<PackageName>libcairo2-dev libgdk-pixbuf2.0-dev</PackageName>
			<FileCheck>/usr/include/cairo/cairo.h, /usr/include/gdk-pixbuf-2.0/gdk-pixbuf/gdk-pixbuf.h</FileCheck>
		</Package>
		<Package>
			<GenericName>xorg-development</GenericName>
			<PackageName>xorg-dev</PackageName>
			<FileCheck>/usr/share/doc/xserver-xorg-dev, /usr/include/xorg</FileCheck>
		</Package>
		<Package>
			<GenericName>bison</GenericName>
			<PackageName>bison</PackageName>
			<FileCheck>/usr/bin/bison</FileCheck>
		</Package>
		<Package>
			<GenericName>flex</GenericName>
			<PackageName>flex</PackageName>
			<FileCheck>/usr/bin/flex</FileCheck>
		</Package>
		<Package>
			<GenericName>imlib2-development</GenericName>
			<PackageName>libimlib2-dev</PackageName>
			<FileCheck>/usr/lib/imlib2</FileCheck>
		</Package>
		<Package>
			<GenericName>java</GenericName>
			<PackageName>openjdk-6-jre</PackageName>
			<FileCheck>/usr/bin/java</FileCheck>
		</Package>
		<Package>
			<GenericName>portaudio-development</GenericName>
			<PackageName>libportaudio-dev</PackageName>
			<FileCheck>/usr/include/portaudio.h</FileCheck>
		</Package>
		<Package>
			<GenericName>fortran-compiler</GenericName>
			<PackageName>gfortran</PackageName>
			<FileCheck>/usr/bin/gfortran</FileCheck>
		</Package>
		<Package>
			<GenericName>glew</GenericName>
			<PackageName>libglew1.5-dev</PackageName>
			<FileCheck>/usr/include/GL/glew.h</FileCheck>
		</Package>
		<Package>
			<GenericName>lib3ds</GenericName>
			<PackageName>lib3ds-dev</PackageName>
			<FileCheck>/usr/include/lib3ds/atmosphere.h</FileCheck>
		</Package>
		<Package>
			<GenericName>freeimage</GenericName>
			<PackageName>libfreeimage3 libfreeimage-dev</PackageName>
			<FileCheck>/usr/lib/libfreeimage.so.3, /usr/include/FreeImage.h</FileCheck>
		</Package>
		<Package>
			<GenericName>scons</GenericName>
			<PackageName>scons</PackageName>
			<FileCheck>/usr/bin/scons</FileCheck>
		</Package>
		<Package>
			<GenericName>zlib-development</GenericName>
			<PackageName>zlib1g-dev</PackageName>
			<FileCheck>/usr/include/zlib.h</FileCheck>
		</Package>
		<Package>
			<GenericName>jpeg-development</GenericName>
			<PackageName>libjpeg-dev</PackageName>
			<FileCheck>/usr/include/jpeglib.h</FileCheck>
		</Package>
		<Package>
			<GenericName>libaio-development</GenericName>
			<PackageName>libaio-dev</PackageName>
			<FileCheck>/usr/include/libaio.h</FileCheck>
		</Package>
		<Package>
			<GenericName>perl</GenericName>
			<PackageName>perl perl-base perl-modules libsdl-perl libperl-dev</PackageName>
			<FileCheck>/usr/bin/perl, /etc/perl/CPAN, /usr/lib/perl5/auto/SDL_perl/SDL_perl.so</FileCheck>
		</Package>
		<Package>
			<GenericName>perl-opengl</GenericName>
			<PackageName>libopengl-perl</PackageName>
			<FileCheck>/usr/lib/perl5/auto/OpenGL/OpenGL.so</FileCheck>
		</Package>
		<Package>
			<GenericName>xorg-video</GenericName>
			<PackageName>libxv-dev libxvmc-dev</PackageName>
			<FileCheck>/usr/include/X11/extensions/Xvlib.h, /usr/include/X11/extensions/XvMClib.h</FileCheck>
		</Package>
		<Package>
			<GenericName>libstdcpp5</GenericName>
			<PackageName>libstdc++5</PackageName>
			<FileCheck>/usr/lib/libstdc++.so.5 OR /usr/lib32/libstdc++.so.5 OR /usr/lib/x86_64-linux-gnu/libstdc++.so.5</FileCheck>
		</Package>
		<Package>
			<GenericName>openal-development</GenericName>
			<PackageName>libopenal-dev</PackageName>
			<FileCheck>/usr/include/AL/al.h</FileCheck>
		</Package>
		<Package>
			<GenericName>vorbis-development</GenericName>
			<PackageName>libvorbis-dev</PackageName>
			<FileCheck>/usr/include/vorbis/vorbisfile.h</FileCheck>
		</Package>
		<Package>
			<GenericName>jam</GenericName>
			<PackageName>jam</PackageName>
			<FileCheck>/usr/bin/jam.perforce</FileCheck>
		</Package>
		<Package>
			<GenericName>p7zip</GenericName>
			<PackageName>p7zip-full</PackageName>
			<FileCheck>/usr/bin/7za</FileCheck>
		</Package>
		<Package>
			<GenericName>qt4-development</GenericName>
			<PackageName>libqt4-dev libqt4-opengl-dev</PackageName>
			<FileCheck>/usr/bin/qmake-qt4, /usr/include/qt4/Qt/qgl.h</FileCheck>
		</Package>
		<Package>
			<GenericName>autoconf</GenericName>
			<PackageName>autoconf</PackageName>
			<FileCheck>/usr/bin/autoconf</FileCheck>
		</Package>
		<Package>
			<GenericName>libtool</GenericName>
			<PackageName>libtool</PackageName>
			<FileCheck>/usr/bin/libtool</FileCheck>
		</Package>
		<Package>
			<GenericName>ncurses-development</GenericName>
			<PackageName>libncurses5-dev</PackageName>
			<FileCheck>/usr/include/ncurses.h</FileCheck>
		</Package>
		<Package>
			<GenericName>popt</GenericName>
			<PackageName>libpopt-dev</PackageName>
			<FileCheck>/usr/include/popt.h</FileCheck>
		</Package>
		<Package>
			<GenericName>numa-development</GenericName>
			<PackageName>libnuma-dev</PackageName>
			<FileCheck>/usr/include/numa.h</FileCheck>
		</Package>
		<Package>
			<GenericName>curl</GenericName>
			<PackageName>libcurl3 curl</PackageName>
			<FileCheck>/usr/bin/curl</FileCheck>
		</Package>
		<Package>
			<GenericName>fftw3-development</GenericName>
			<PackageName>libfftw3-dev fftw-dev</PackageName>
			<FileCheck>/usr/include/fftw3.h</FileCheck>
		</Package>
		<Package>
			<GenericName>blas-development</GenericName>
			<PackageName>libblas-dev</PackageName>
			<FileCheck>/usr/include/cblas.h</FileCheck>
		</Package>
		<Package>
			<GenericName>lapack-development</GenericName>
			<PackageName>liblapack-dev</PackageName>
			<FileCheck>/usr/lib/liblapack.so</FileCheck>
		</Package>
		<Package>
			<GenericName>atlas-development</GenericName>
			<PackageName>libatlas-base-dev</PackageName>
			<FileCheck>/usr/lib/libatlas.a</FileCheck>
		</Package>
		<Package>
			<GenericName>openmpi-development</GenericName>
			<PackageName>libopenmpi-dev openmpi-bin libmpich2-dev libmpich1.0-dev</PackageName>
			<FileCheck>/usr/lib/openmpi/lib/libmpi.so, /usr/bin/mpirun.openmpi</FileCheck>
		</Package>
		<Package>
			<GenericName>cmake</GenericName>
			<PackageName>cmake</PackageName>
			<FileCheck>/usr/bin/cmake</FileCheck>
		</Package>
		<Package>
			<GenericName>boost-development</GenericName>
			<PackageName>libboost-all-dev libasio-dev</PackageName>
			<FileCheck>/usr/include/boost/, /usr/include/asio/, /usr/include/boost/date_time/, /usr/include/boost/regex.hpp, /usr/include/boost/iostreams/write.hpp</FileCheck>
		</Package>
		<Package>
			<GenericName>bzip2-development</GenericName>
			<PackageName>libbz2-dev</PackageName>
			<FileCheck>/usr/include/bzlib.h</FileCheck>
		</Package>
		<Package>
			<GenericName>tcl</GenericName>
			<PackageName>tcl</PackageName>
			<FileCheck>/usr/bin/tclsh</FileCheck>
		</Package>
		<Package>
			<GenericName>glibc-development</GenericName>
			<PackageName>libc6-dev</PackageName>
			<FileCheck>/usr/include/gnu/stubs-32.h OR /usr/include/gnu-versions.h</FileCheck>
		</Package>
		<Package>
			<GenericName>glibc-development</GenericName>
			<PackageName>libc6-dev-i386</PackageName>
			<FileCheck>/usr/lib32/libc.so</FileCheck>
			<ArchitectureSpecific>x86_64</ArchitectureSpecific>
		</Package>
		<Package>
			<GenericName>python</GenericName>
			<PackageName>python</PackageName>
			<FileCheck>/usr/bin/python</FileCheck>
		</Package>
		<Package>
			<GenericName>python-boost-development</GenericName>
			<PackageName>libboost-python-dev</PackageName>
			<FileCheck>/usr/include/boost/python.hpp</FileCheck>
		</Package>
		<Package>
			<GenericName>boost-thread-development</GenericName>
			<PackageName>libboost-thread-dev</PackageName>
			<FileCheck>/usr/include/boost/thread.hpp</FileCheck>
		</Package>
		<Package>
			<GenericName>python-numpy</GenericName>
			<PackageName>python-numpy</PackageName>
			<FileCheck>/usr/share/pyshared/numpy/version.py</FileCheck>
		</Package>
		<Package>
			<GenericName>yasm</GenericName>
			<PackageName>yasm</PackageName>
			<FileCheck>/usr/bin/yasm</FileCheck>
		</Package>
		<Package>
			<GenericName>gmp-library</GenericName>
			<PackageName>libgmp3-dev</PackageName>
			<FileCheck>/usr/include/gmp.h</FileCheck>
		</Package>
	</ExternalDependencies>
</PhoronixTestSuite>
