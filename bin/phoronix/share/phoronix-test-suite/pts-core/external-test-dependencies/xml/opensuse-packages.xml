		<Information>
			<Name>OpenSuSE</Name>
			<Aliases>SUSE Linux, SUSE</Aliases>
		</Information>
		<Package>
			<GenericName>gtk-development</GenericName>
			<PackageName>gtk2-devel</PackageName>
			<FileCheck>/usr/include/gtk-2.0</FileCheck>
		</Package>
